{
  "name": "nu-headlines",
  "version": "1.1.0",
  "description": "Get the latest headlines from Dutch news website nu.nl inside your terminal",
  "repository": "github:hendriklammers/nu-headlines",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "start": "nodemon --watch src/**/*.ts --exec ts-node src/index.ts"
  },
  "keywords": [
    "node",
    "typescript",
    "nu.nl",
    "cli",
    "news"
  ],
  "author": "Hendrik Lammers <lammers@gmail.com> (http://www.hendriklammers.com)",
  "license": "MIT",
  "dependencies": {
    "@types/xml2js": "^0.4.3",
    "axios": "^0.18.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "inquirer": "^6.2.2",
    "opn": "^5.4.0",
    "ora": "^3.1.0",
    "rss-parser": "^3.6.2"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.10",
    "@types/inquirer": "0.0.43",
    "@types/node": "^11.9.4",
    "@types/opn": "^5.1.0",
    "@types/ora": "^3.0.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "bin": {
    "nu.nl": "lib/index.js"
  },
  "files": [
    "lib/**/*"
  ]
}
